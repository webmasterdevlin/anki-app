{
  "productName": "devlin",
  "version": "0.0.0",
  "identifier": "com.tauri.devlin",
  "build": {
    "beforeDevCommand": "bun dev",
    "devUrl": "http://localhost:5173",
    "beforeBuildCommand": "bun build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "devlin",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": ["icons/32x32.png", "icons/128x128.png", "icons/128x128@2x.png", "icons/icon.icns", "icons/icon.ico"]
  }
}
