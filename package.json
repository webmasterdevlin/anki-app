{
  "name": "anki-app",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "generate-assets": "pwa-assets-generator --preset minimal public/logo.png",
    "generate-assets-2023": "pwa-assets-generator --preset minimal-2023 public/logo.png",
    "eslint": "eslint",
    "checkTs": "tsc --noEmit",
    "prepare": "husky",
    "tauri": "tauri",
    "init:android": "tauri android init",
    "init:ios": "tauri ios init",
    "dev:android": "RUST_BACKTRACE=1 tauri android dev",
    "dev:ios": "RUST_BACKTRACE=1 tauri ios dev 'iPhone 16'",
    "build:desktop": "tauri build --target aarch64-apple-darwin"
  },
  "dependencies": {
    "@react-spring/web": "^9.7.5",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/postcss": "^4.1.3",
    "@tailwindcss/typography": "^0.5.16",
    "@tanstack/react-router": "^1.115.2",
    "@tauri-apps/api": "^2.4.1",
    "@tauri-apps/plugin-barcode-scanner": "^2.2.0",
    "@tauri-apps/plugin-dialog": "^2.2.1",
    "axios": "^1.8.4",
    "clsx": "^2.1.1",
    "internal-ip": "7.0.0",
    "logrocket": "^9.0.2",
    "logrocket-react": "6.0.3",
    "million": "^3.1.11",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "react": "^18.3.1",
    "react-confetti": "^6.4.0",
    "react-dom": "^18.3.1",
    "react-icons": "^5.5.0",
    "react-joyride": "^2.9.3",
    "react-scan": "^0.3.3",
    "react-toastify": "^11.0.5",
    "react-use": "^17.6.0",
    "tailwind-merge": "^3.2.0",
    "tailwindcss": "^4.1.3",
    "vite-plugin-eslint": "^1.8.1"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.8",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.24.0",
    "@tanstack/router-devtools": "^1.115.2",
    "@tanstack/router-vite-plugin": "^1.115.2",
    "@tauri-apps/cli": "^2.4.1",
    "@types/node": "^22.14.0",
    "@types/react": "^18.3.20",
    "@types/react-dom": "^18.3.6",
    "@typescript-eslint/eslint-plugin": "^8.29.1",
    "@typescript-eslint/parser": "^8.29.1",
    "@vite-pwa/assets-generator": "^1.0.0",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-import-resolver-typescript": "^4.3.2",
    "eslint-plugin-autofix": "^2.2.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.0",
    "typescript": "^5.8.3",
    "vite": "^6.2.5",
    "vite-plugin-pwa": "^1.0.0",
