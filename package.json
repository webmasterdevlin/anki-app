{
  "name": "anki-app",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "generate-assets": "pwa-assets-generator --preset minimal public/logo.png",
    "generate-assets-2023": "pwa-assets-generator --preset minimal-2023 public/logo.png",
    "eslint": "eslint",
    "checkTs": "tsc --noEmit",
    "prepare": "husky",
    "tauri": "tauri",
    "init:android": "tauri android init",
    "init:ios": "tauri ios init",
    "dev:android": "RUST_BACKTRACE=1 tauri android dev",
    "dev:ios": "RUST_BACKTRACE=1 tauri ios dev",
    "build:desktop": "tauri build --target aarch64-apple-darwin"
  },
  "dependencies": {
    "@react-spring/web": "^9.7.4",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.9",
    "@tailwindcss/typography": "^0.5.15",
    "@tanstack/react-router": "^1.56.5",
    "@tauri-apps/api": "2.0.0-rc.4",
    "@tauri-apps/plugin-barcode-scanner": "^2.0.0-rc.1",
    "@tauri-apps/plugin-dialog": "2.0.0-rc.1",
    "@tauri-apps/plugin-shell": "2.0.0-rc.1",
    "autoprefixer": "^10.4.20",
    "axios": "^1.7.7",
    "clsx": "^2.1.1",
    "internal-ip": "7.0.0",
    "logrocket": "^9.0.0",
    "logrocket-react": "6.0.3",
    "million": "latest",
    "postcss": "^8.4.45",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.6",
    "react": "^18.3.1",
    "react-confetti": "^6.1.0",
    "react-dom": "^18.3.1",
    "react-icons": "^5.3.0",
    "react-joyride": "^2.8.2",
    "react-toastify": "^10.0.5",
    "react-use": "^17.5.1",
    "tailwind-merge": "^2.5.2",
    "tailwindcss": "^3.4.10",
    "vite-plugin-eslint": "^1.8.1"
  },
  "devDependencies": {
    "@eslint/compat": "^1.1.1",
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.10.0",
    "@tanstack/router-devtools": "^1.56.5",
    "@tanstack/router-vite-plugin": "^1.56.4",
    "@tauri-apps/cli": "^2.0.0-rc.12",
    "@types/node": "^22.5.4",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.4.0",
    "@typescript-eslint/parser": "^8.4.0",
    "@vite-pwa/assets-generator": "^0.2.6",
    "@vitejs/plugin-react": "^4.3.1",
    "eslint": "^9.10.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-autofix": "^2.2.0",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-jsx-a11y": "^6.10.0",
    "eslint-plugin-react": "^7.35.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "globals": "^15.9.0",
    "husky": "^9.1.5",
    "lint-staged": "^15.2.10",
